<!-- https://docs.microsoft.com/en-us/visualstudio/msbuild/customize-your-build?view=vs-2022#directorybuildprops-and-directorybuildtargets -->
<Project>
 <PropertyGroup>
   <Deterministic>true</Deterministic>
	 <ApplicationIcon>$(MSBuildThisFileDirectory)Images\favicon.ico</ApplicationIcon>
	 <Authors>Hans Dickel</Authors>
	 <Company>Aeriden</Company>
	 <Copyright>Copyright © 2025, All Rights Reserved</Copyright>
	 <Description>Azure DevOps Boards Project Planning</Description>
	 <PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
	 <PackageProjectURL>https://github.com/geekfog/AzDoBoards</PackageProjectURL>
	 <PackageIcon>favicon-0128.png</PackageIcon>
	 <Product>AzDoBoards</Product>
	 <RepositoryType>Git</RepositoryType>
	 <RepositoryUrl>https://github.com/geekfog/AzDoBoards.git</RepositoryUrl>
	 <VersionPrefix>01.10.01</VersionPrefix>
 </PropertyGroup>

  <ItemGroup>
	<Content Include="$(MSBuildThisFileDirectory)Images\favicon.ico">
	  <Link>Images\favicon.ico</Link>
	</Content>
  </ItemGroup>

  <ItemGroup>
	<None Include="$(MSBuildThisFileDirectory)Images\favicon-0128.png" Pack="True" PackagePath="">
	  <Link>Images\favicon-0128.png</Link>
	</None>
  </ItemGroup>
</Project>